#include <iostream>
using namespace std;
int min1(int a, int b)
{
   if (a >= b)
   {
       return b;
   }
    else
    {
        return a;
    }
}
int min2(int c, int d)
{
    if(c >= d)
    {
        return d;
    }
    else
    {
        return c;
    }
}
int result(int x, int y)
{
    int a,b,c,d;
    x = min1(a , b);
    y = min2(c , d);
    if(x >= y)
    {
        return y;
    }
    else
    {
        return x;
    }
}

int main() {
    int a , b, c, d,x,y;
    cin>>a>>b>>c>>d;
    min1(a, b);
    min2(c, d);
    cout<<result(x ,y);
    
  return 0;
}
